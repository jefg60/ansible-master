inventories = [{% for inv in anmad_inventories %}{{ inv }}, {% endfor %}]
playbook_root_dir = {{ anmad_root }}
playbooks = [{% for play in anmad_playbooks %}{{ play }}, {% endfor %}]
venv = {{ ara_override.install.pip.virtualenv_path }}
{% if syntax_check_dir is defined %}
syntax_check_dir = {{ syntax_check_dir }}
{% endif %}
{% if anmad_prerun is defined %}
pre_run_playbooks = [{% for play in anmad_prerun %}{{ play }}, {% endfor %}]
{% endif %}
{% if master_ansible_user_vaultfile is defined %}
ssh_askpass = /usr/bin/ssh-askpass
{% endif %}
{% if anmad_ara_url is defined %}
ara_url = {{ anmad_ara_url }}
{% endif %}
{% if anmad_debug is defined %}
debug = True
{% endif %}
